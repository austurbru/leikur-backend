{
  "kind": "collectionType",
  "collectionName": "levels",
  "info": {
    "name": "level",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "levelNo": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "description": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "lessons": {
      "via": "level",
      "collection": "lesson"
    },
    "usersCurrentCourse": {
      "via": "currentCourse",
      "plugin": "users-permissions",
      "collection": "user"
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "Yellow",
        "Red",
        "Green",
        "Blue"
      ]
    }
  }
}
